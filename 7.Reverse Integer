class Solution():
    def reverse(self,x):

        total = 0
        temp = x if x>=0 else x*-1
        while temp > 0:
            total = total*10 + temp % 10
            temp //= 10
        
        total = total if x>=0 else total*-1
        
        if total <-2**31 or total >2**31-1:
            return 0
        else:
            return total 
        
        


if __name__ == "__main__":
    i = Solution()
    print (i.reverse(-2147483412))
